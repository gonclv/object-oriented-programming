package aula02;

public class ContaBancariaTeste {

	public static void main(String[] args) {
		ContaBancaria conta1 = new ContaBancaria(111, "Jo√£o da Silva");
		
		conta1.imprimirSaldo();
		conta1.depositar(100.00);
		conta1.imprimirSaldo();
		conta1.sacar(110.00);
		conta1.imprimirSaldo();
		
		conta1.setNomeTitular("Ana Maria");
		conta1.setNumeroConta(222);
		conta1.setSaldo(1000.00);
		
		System.out.println(conta1.getNumeroConta());
		System.out.println(conta1.getNomeTitular());
		System.out.println(conta1.getSaldo());
	}
}
